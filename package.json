{
  "name": "reto_fracttal",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:report": "cypress run --reporter cypress-mochawesome-reporter",
    "test:headed": "cypress run --headed --no-exit",
    "test:report:headed": "cypress run --headed --no-exit --reporter cypress-mochawesome-reporter",
    "merge:reports": "mochawesome-merge cypress/reports/*.json > cypress/reports/merged-report.json",
    "generate:report": "marge cypress/reports/merged-report.json --reportDir cypress/reports --assetsDir cypress/reports/assets --reportFilename reporte-final.html",
    "test:full": "npm run test:report && npm run merge:reports && npm run generate:report",
    "clean:reports": "rimraf cypress/reports && mkdir cypress/reports"
  },
  "author": "Alejandro caro",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^14.5.4",
    "cypress-mochawesome-reporter": "^3.8.2",
    "dotenv": "^17.2.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "rimraf": "^6.0.1"
  }
}
